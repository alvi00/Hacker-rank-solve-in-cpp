#include<bits/stdc++.h>
using namespace std;
int main()
{
   int a=0,b=0;
   int alice[3];
   int bob[3];
   for(int i=0;i<3;i++)
   {
      cin>>alice[i];
   } 

      for(int i=0;i<3;i++)
   {
      cin>>bob[i];
   } 

   for(int i=0;i<3;i++)
   {
      if(alice[i]>bob[i])
      {
         a++;
      }
      else if(alice[i]<bob[i])
      {
         b++;
      }
   } 

   cout<<a<<" "<<b;
     
   


}
