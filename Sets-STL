#include<bits/stdc++.h>
using namespace std;
 set<int>s;
int main()
{
  int t;
  cin>>t;
  for(int i=0;i<t;i++)
  {
    int a,b;
    cin>>a>>b;
   
  
    if(a==1)
    {
        s.insert(b);
    }
    if(a==2)
    {
        s.erase(b);
    }
    if(a==3)
    {
           set<int>::iterator itr=s.find(b);
            if(itr != s.end())
            {
                cout<<"Yes"<<endl;
            }
            else
            {
                cout<<"No"<<endl;
            }
        }
  
    }
  }
